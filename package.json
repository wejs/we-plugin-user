{
  "name": "we-plugin-user",
  "description": "We.js user resource and routes",
  "version": "2.0.0",
  "main": "plugin.js",
  "scripts": {
    "test": "NODE_ENV=test LOG_LV=info ./node_modules/.bin/mocha test/bootstrap.js test/**/*.test.js",
    "coverage": "NODE_ENV=test LOG_LV=info node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports -R spec test/bootstrap.js test/**/*.test.js"
  },
  "devDependencies": {
    "express-mysql-session": "^1.2.3",
    "fs-extra": "^5.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.2",
    "rimraf": "^2.6.2",
    "supertest": "^3.0.0",
    "we-core": "^2",
    "we-plugin-acl": "wejs/we-plugin-acl",
    "we-plugin-auth": "wejs/we-plugin-auth",
    "we-test-tools": "wejs/we-test-tools"
  },
  "keywords": [
    "wejs-plugin",
    "user"
  ],
  "files": [
    "commands",
    "files",
    "lib",
    "locales",
    "server",
    "cron.js",
    "install.js",
    "plugin.js"
  ],
  "repository": "wejs/we-plugin-user",
  "author": "Alberto Souza <contato@albertosouza.net>",
  "license": "MIT",
  "wejs": {
    "plugins": {
      "we-plugin-auth": true,
      "we-plugin-acl": true
    },
    "devPlugins": {}
  }
}
